{
  "name": "@finqu/aptl",
  "version": "1.0.0-alpha.25",
  "description": "A template engine for rendering human-readable templates with simple conditionals and easy data injection.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./local-filesystem": {
      "import": {
        "types": "./dist/types/filesystem/local-filesystem.d.ts",
        "default": "./dist/filesystem/local-filesystem.mjs"
      },
      "require": {
        "types": "./dist/types/filesystem/local-filesystem.d.ts",
        "default": "./dist/filesystem/local-filesystem.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "test": "jest",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm build",
    "version:patch": "./scripts/version.sh patch",
    "version:minor": "./scripts/version.sh minor",
    "version:major": "./scripts/version.sh major",
    "version:prerelease": "./scripts/version.sh prerelease",
    "version:publish": "./scripts/version.sh patch --publish",
    "release": "./scripts/version.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finqu/aptl.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "template",
    "engine",
    "rendering",
    "conditional",
    "data-injection",
    "ai"
  ],
  "author": "Finqu",
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.0.6",
    "prettier": "^3.6.2",
    "rolldown": "1.0.0-beta.43",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.4"
  }
}
